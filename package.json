{
    "name": "ng-all-in-one",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "nx serve bed-rock-doc --hmr",
        "start:storybook": "nx run bed-rock-doc:storybook",
        "start:@kidwen/bed-rock": "nx run @kidwen/bed-rock:storybook",
        "doc": "nx storybook bed-rock-doc",
        "doc:bed-rock": "nx storybook @kidwen/bed-rock",
        "build:bed-rock-doc": "nx build bed-rock-doc",
        "build:bed-rock": "nx build @kidwen/bed-rock",
        "build:all": "nx run @kidwen/bed-rock:build-storybook && nx run bed-rock-doc:build-storybook",
        "watch": "nx build bed-rock-doc --watch --configuration development",
        "test": "nx test bed-rock-doc",
